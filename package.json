{
  "name": "style-transform-saas",
  "version": "1.0.0",
  "description": "AI-powered image style transformation platform",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd backend && npm run dev",
    "client": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-all": "cd frontend && npm install && cd ../backend && npm install",
    "start": "cd backend && npm start"
  },
  "keywords": [
    "ai",
    "image-processing",
    "style-transfer",
    "saas",
    "react",
    "nodejs"
  ],
  "author": "Radincuyy",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}